#include <bits/stdc++.h>
using namespace std;
void make(int array[], int size)
{
    
    int check = 1;
    int divide;
    for (int i = 0; i < count; i++)
    {
        if(i == 0)
        {array[0] = 1; continue;}

       if(i % 2 == 0)
       //even
       {
           for (int k = 0; k < size; k++)
           {
               /* code */
           }
       }
       else
       //odd
       {
           for (int j = check; j < size; j++)
           {
               if (j / array[i] == 0)
               {
                   array[i + 1] = j;
               }
               
           }
           
       }
    }
    
}
void print(int array[], size)
{
    for (int i = 0; i < size; i++)
    {
       cout << array[i];
    }
    
}
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int size;
        cin >> size;
        int arr[size];
        make(arr, size);
        print(arr, size);
        
    }
    return 0;
}